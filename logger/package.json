  "homepage": "https://openkfw.github.io/trubudget-website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openkfw/TruBudget.git"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@size-limit/time": "^4.10.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "codecov": "^3.8.1",
