{
  "name": "trubudget-logger",
  "version": "0.0.0",
  "licence": "MIT",
  "author": {
    "name": "name",
    "email": "email@domain.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "esnext": "dist/esm/index.mjs",
  "description": "A simple logger for TruBudget",
  "keywords": [
    "typescript",
    "trubudget",
    "openkfw"
  ],
  "scripts": {
