    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-check": "^5.9.2",
    "pino": "^7.0.0-rc.2",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "size-limit": "^4.10.2",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typedoc-plugin-markdown": "^3.6.1",
    "typescript": "^4.2.3"
  }
